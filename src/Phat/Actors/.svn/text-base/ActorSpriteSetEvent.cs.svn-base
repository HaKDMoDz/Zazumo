using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using Phat;

namespace Phat.Actors
{
    public class ActorSpriteSetEvent : ActorEvent
    {
        public String SpriteKey { get; private set; }

        public ActorSpriteSetEvent(Guid actorId, String spriteKey)
            : base(actorId)
        {
            this.SpriteKey = spriteKey;
        }
    }
}
